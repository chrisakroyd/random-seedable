{
  "name": "random-seedable",
  "version": "1.0.1",
  "description": "Fully-fledged collection of seedable random number generators whose output precisely matches their original c/c++ implementations.",
  "keywords": [
    "Xorwow",
    "Xorshift",
    "LCG",
    "random",
    "number",
    "generator",
    "seedable",
    "random-seedable",
    "PCG",
    "Mersenne",
    "Twister",
    "pseudorandom",
    "prng",
    "rng"
  ],
  "main": "src/index.js",
  "type": "module",
  "directories": {
    "test": "test",
    "src": "src",
    "reference": "reference",
    "benchmark": "benchmark",
    "example": "examples"
  },
  "scripts": {
    "benchmark": "node benchmark/index.js",
    "test": "mocha test/*.js",
    "coverage": "nyc mocha --experimental-loader=@istanbuljs/esm-loader-hook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisAkroyd/random-seedable.git"
  },
  "author": "Chris Akroyd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChrisAkroyd/random-seedable/issues"
  },
  "homepage": "https://github.com/ChrisAkroyd/random-seedable#readme",
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "src/*.js"
  ]
}
